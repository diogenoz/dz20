<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>

    <g:DecoratorPanel>
        <g:FormPanel>
            <g:VerticalPanel spacing="10">
                <g:HorizontalPanel>
                    <g:Label text="ID:" width="100px"/>
                    <g:TextBox enabled="false" ui:field="idText" width="200px"/>
                </g:HorizontalPanel>
                <g:HorizontalPanel>
                    <g:Label text="Name:" width="100px"/>
                    <g:TextBox name="name" ui:field="nameText" width="200px"/>
                </g:HorizontalPanel>
                <g:HorizontalPanel>
                    <g:Label text="Age:" width="100px"/>
                    <g:TextBox name="age" ui:field="ageText" width="200px"/>
                </g:HorizontalPanel>

                <g:Button ui:field="btnSubmit">Создать</g:Button>
            </g:VerticalPanel>
        </g:FormPanel>
    </g:DecoratorPanel>
</ui:UiBinder>